{
  "name": "sanctuary-api",
  "version": "0.1.0",
  "description": "Sanctuary API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "migrate": "knex migrate:latest --knexfile knexfile.js",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "test": "eslint src",
    "start:prod": "npm run migrate && npm start"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^8.2.0",
    "express": "^4.13.3",
    "knex": "^0.20.10",
    "moment": "^2.24.0",
    "morgan": "^1.8.0",
    "mustache": "^3.1.0",
    "mysql": "^2.1.0",
    "resource-router-middleware": "^0.6.0",
    "twilio": "^3.39.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.7.6",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  }
}
